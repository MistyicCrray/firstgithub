			password="123456">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator
			targetPackage="com.springboot.entity"
			targetProject="E:/generator/firstgithub/first-spring-boot_01/src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.springboot.entity"
			targetProject="E:/generator/firstgithub/first-spring-boot_01/src/main/java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="ANNOTATEDMAPPER"
			targetPackage="com.springboot.mapper"
			targetProject="E:/generator/firstgithub/first-spring-boot_01/src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>
